@model IEnumerable<IdentityApp.Models.ApplicationUser>
@{
    ViewBag.Title = "Список пользователей";
}

<a asp-controller="Users" asp-action="UnBlock">Разблокировать</a>
<a asp-controller="Users" asp-action="Block">Заблокировать</a>
<a asp-controller="Users" asp-action="Delete">Удалить</a>
<a asp-controller="Users" asp-action="SelectAll">Выбрать всех</a>
<a asp-controller="Users" asp-action="RemoveAll">Снять выделение</a>






    <table class="table">
        <tr><th>CheckBoxes</th><th>Email</th><th>LastLoginDate</th><th>LastRegistrationDate</th><th>Name</th><th>Status</th><th>Id</th></tr>

        @foreach (var user in Model)
        {
    <tr>
        <td><input type="checkbox"  asp-for="@user.IsChecked" class="checkboxClass" data-number="@user.Id"/> </td>
        <td>@user.Email</td>
        <td>@user.LastLoginDate</td>
        <td>@user.LastRegistrationDate</td>
        <td>@user.Name</td>
        <td>@user.Status</td>
        <td>@user.Id</td>
    </tr>
        }
    </table>



